msgstr "Mestre"

msgctxt "#32009"
msgid "Slave"
msgstr "Escravo"

msgctxt "#32010"
msgid "Update interval in hours (0 = at startup only)"
msgstr "Intervalo de actualização em horas (0 = apenas no início)"

